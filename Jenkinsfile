pipeline {
  agent any
  stages {
    stage('Install_Terraform') {
      steps {
        sh '''chmod 777 /root/*
touch 4498.txt
echo "Hello Blue Ocean" 
echo "Hello Blue Ocean" > 4498.txt'''
      }
    }

    stage('Success') {
      steps {
        sh 'Terraform successfully installed. '
      }
    }

  }
}